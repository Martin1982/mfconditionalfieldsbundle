{% block conditional_field_js_row %}
    <script type="text/javascript">
        {%- set debug = 'false' %}
        {%- if app.request.server.get('APP_ENV') == 'dev' %}
            {%- set debug = 'true' %}
        {%- endif %}
        if (typeof mfConditionalFields === 'undefined') {
            console.error('mfConditionalFields is not defined, did you load the JS library?');
        } else {
            mfConditionalFields('{{ form_id }}', {
                debug: {{ debug }}
            });
        }
    </script>
{% endblock %}